	Response create(${resourceRepresentation.name} rro);
	Response getBy${keyPropertyName}(${keyPropertyType} ${keyName});
	Response removeBy${keyPropertyName}(${keyPropertyType} ${keyName});
	Response update(${keyPropertyType} ${keyName}, ${resourceRepresentation.name} rro);
}